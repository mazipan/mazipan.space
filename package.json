{
  "name": "mazipan.space",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "astro": "astro",
    "build:with-version": "APP_VERSION=$(npm pkg get version | xargs) APP_DEPLOY_AT=$(date +'%Y-%m-%d %H:%M:%S') astro build",
    "build": "yarn generate && yarn build:with-version",
    "check-types": "tsc --noEmit --pretty",
    "clean": "rm -rf node_modules yarn.lock",
    "dev": "APP_VERSION=$(npm pkg get version | xargs) APP_DEPLOY_AT=$(date +'%Y-%m-%d %H:%M:%S') astro dev",
    "format": "prettier --write .",
    "lint": "eslint --ext .astro,.tsx,.ts,.js,.mdx src",
    "preview": "astro preview",
    "start": "serve ./dist",
    "generate": "node ./scripts/copy-talks.js && node ./scripts/copy-awesome-sde.js",
    "sync": "astro sync"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.8",
    "@astrojs/partytown": "^2.1.3",
    "@astrojs/react": "^4.2.0",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^6.0.0",
    "@expressive-code/core": "^0.40.1",
    "@expressive-code/plugin-line-numbers": "^0.40.1",
    "@fontsource-variable/jetbrains-mono": "^5.1.2",
    "@fontsource-variable/space-grotesk": "^5.1.1",
    "@fontsource/dela-gothic-one": "^5.1.1",
    "astro": "5.2.1",
    "astro-embed": "^0.9.0",
    "astro-expressive-code": "^0.40.1",
    "astro-icon": "^1.1.5",
    "astro-remote": "^0.3.3",
    "astro-social-share": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "expressive-code-color-chips": "^0.1.2",
    "feed": "^4.2.2",
    "giscus": "^1.6.0",
    "github-slugger": "^2.0.0",
    "mdast-util-to-string": "^4.0.0",
    "object-treeify": "^5.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reading-time": "^1.5.0",
    "rehype-code-group": "^0.2.3",
    "sharp": "0.33.5",
    "tailwind-clip-path": "^1.0.0",
    "tailwind-merge": "^3.0.1",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@expressive-code/plugin-collapsible-sections": "^0.40.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@iconify-json/tabler": "^1.2.15",
    "@tailwindcss/typography": "^0.5.16",
    "@types/mdast": "^4.0.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-mdx": "^3.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-mdx": "^3.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "satori": "^0.12.1",
